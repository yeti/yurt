{
  "name": "backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "generate": "pnpm generate:prisma && pnpm generate:nexus",
    "generate:nexus": "tsx src/schema.ts",
    "generate:prisma": "prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:create": "pnpm prisma migrate dev --create-only",
    "dev": "nodemon",
    "lint": "eslint --ext .ts src",
    "migrate:deploy": "prisma migrate deploy",
    "build": "pnpm install --prod=false && pnpm generate && tsc && resolve-tspaths && pnpm migrate:deploy",
    "test": "NODE_ENV=test pnpm migrate:test && pnpm run-test",
    "seed": "tsx prisma/seed.ts",
    "run-test": "DATABASE_URL=postgresql://prisma:prisma@localhost:5434/tests jest --runInBand",
    "migrate:test": "DATABASE_URL=postgresql://prisma:prisma@localhost:5434/tests pnpm prisma migrate dev",
    "db:test:start": "docker-compose -f docker-compose.jest.yml down && docker-compose -f docker-compose.jest.yml up -d"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "^5.11.0",
    "@sentry/node": "^7.80.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-shield": "^7.6.5",
    "helmet": "^7.1.0",
    "nanoid": "^3.3.7",
    "nexus": "^1.3.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.28",
    "@swc/helpers": "^0.5.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.11",
    "@types/node": "20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.56.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "nodemon": "^3.0.1",
    "prisma": "^5.11.0",
    "regenerator-runtime": "^0.14.0",
    "resolve-tspaths": "^0.8.15",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-jest-mock-import-meta": "^1.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
